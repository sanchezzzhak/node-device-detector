    "@fast-csv/parse": "^5.0.0",
    "@fast-csv/format": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "benchmark": "^2.1.4",
    "chai": "^4.5.0",
    "cli-table": "^0.3.6",
    "commander": "^11.1.0",
    "eslint": "^8.27.0",
    "mocha": "^10.8.2",
    "prettier": "^2.4.1",
    "typescript": "^4.8.4"
  }
